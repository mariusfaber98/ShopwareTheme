                        with { flareHeaderActionCssClasses : theme_config('flare-header-second-action-left-css-classes') }
                    %}
                {% endif %}
            {% endblock %}

            {% block layout_flare_header_third_action_left %}
                {% if theme_config('flare-header-third-action-left') is not same as('empty') %}
                    {% sw_include '@Storefront/storefront/flaretheme/layout/header/actions/flare-header-action-' ~ theme_config('flare-header-third-action-left') ~ '.html.twig' ignore missing
                        with { flareHeaderActionCssClasses : theme_config('flare-header-third-action-left-css-classes') }
                    %}
                {% endif %}
            {% endblock %}

            {% block layout_flare_header_fourth_action_left %}
                {% if theme_config('flare-header-fourth-action-left') is not same as('empty') %}
                    {% sw_include '@Storefront/storefront/flaretheme/layout/header/actions/flare-header-action-' ~ theme_config('flare-header-fourth-action-left') ~ '.html.twig' ignore missing
                        with { flareHeaderActionCssClasses : theme_config('flare-header-fourth-action-left-css-classes') }
                    %}
                {% endif %}
            {% endblock %}

            {% block layout_flare_header_fifth_action_left %}
                {% if theme_config('flare-header-fifth-action-left') is not same as('empty') %}
                    {% sw_include '@Storefront/storefront/flaretheme/layout/header/actions/flare-header-action-' ~ theme_config('flare-header-fifth-action-left') ~ '.html.twig' ignore missing
                        with { flareHeaderActionCssClasses : theme_config('flare-header-fifth-action-left-css-classes') }
                    %}
                {% endif %}
            {% endblock %}
        </div>
    </div>
{% endblock %}
